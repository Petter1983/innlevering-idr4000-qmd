@article{benekaResistanceTrainingEffects2005,
  title = {Resistance Training Effects on Muscular Strength of Elderly Are Related to Intensity and Gender},
  author = {Beneka, A and Malliou, P and Fatouros, I and Jamurtas, A and Gioftsidou, A and Godolias, G and Taxildaris, K},
  year = {2005},
  month = aug,
  journal = {Journal of Science and Medicine in Sport},
  volume = {8},
  number = {3},
  pages = {274--283},
  issn = {14402440},
  doi = {10.1016/S1440-2440(05)80038-6},
  urldate = {2023-11-02},
  abstract = {The purpose of this study was to determine whether a high intensity (HI) versus a moderate (MI) or low-intensity (LI) training program would be more effective in improving the isokinetic knee extension muscular performance in heal|hy inaciive men and women. Sixty-four participants, men and women, were randomly assigned to one of four groups: control group (C), LI (50\% of 1RM), the MI group (70\% of 1RM) and the HI (90\% of 1RM). Participants exercised on three resistance exercise machines: leg extension, leg curls and leg press. The isokinetic testing method (concentric mode) applied prior to and at the end of the training period (16 weeks, three 3 times per week) to assess the knee muscular performance. In conclusion, HI strength training is proposed for elderly men and women as the most effective protocol.},
  langid = {english},
  note = {The purpose of this study was to determine whether a high intensity (HI) versus a moderate (MI) or low-intensity (LI) training program would be more effective in improving the isokinetic knee extension muscular performance in heal|hy inaciive men and women. Sixty-four participants, men and women, were randomly assigned to one of four groups: control group (C), LI (50\% of 1RM), the MI group (70\% of 1RM) and the HI (90\% of 1RM). Participants exercised on three resistance exercise machines: leg extension, leg curls and leg press. The isokinetic testing method (concentric mode) applied prior to and at the end of the training period (16 weeks, three 3 times per week) to assess the knee muscular performance.
\par
In conclusion, HI strength training is proposed for elderly men and women as the most effective protocol.
\par
INtro:
\par
\guillemotleft Muscle strength decreases by 1\%-2\% per year in people over 65 y at a greater rate for lower extremity than for the upper extremity 1,2. Current evidence suggests knee extensor strength loss in the elderly is reversed by exercisea,4,5. Training-induced strength gains have been observed with several different types of training: isokinetic, isometric and isotonic exercise4,6,7. Programs are administered with a frequency of 2-3 times/wk and duration of 8-24 weeks\guillemotright{} (Beneka et al., 2005, p. 274)
\par
M\aa l med studien:
\par
\guillemotleft The first aim of the present study was to determine the effects of strength training intensity on strength development of healthy inactive elderly after a training period of 16 weeks.
\par
The second aim was to test the hypothesis that the magnitude of the intensityrelated effects are related to the joint's angular velocity, and the third aim was to examine whether or not resistance training produces gender-specific adaptations.\guillemotright{} (Beneka et al., 2005, p. 275)
\par
\guillemotleft Statistical analysis Data were analysed using the SPSS PC program for Windows (version 8). Means and standard deviations (+SD) were calculated. One-way ANOVA was conducted to determine differences between groups at baseline for each variable. MANOVA\guillemotright{} (Beneka et al., 2005, p. 276)
\par
Resultat:
\par
\guillemotleft Results of this study revealed that the HI training protocol caused the most impressive improvement of the knee extensors' isokinetic performance as compared to LI or MI at all testing velocities. However, moderate- and lowintensity training groups caused a significant strength improvement as well, but at a lesser extent. Specifically, the male participants in HI increased their performance by 7.3\%- 11.2\% and females by 2.3\%- 15.2\%. In MI, improvement ranged between 6.4\% and 9.2\% for males and 1.9\% to 7.9\% for females respectively. In LI the corresponding numbers were 2.2\%-5.2\% for males and 2.6\%-3.7\% for females.\guillemotright{} (Beneka et al., 2005, p. 279)},
  file = {files/294/Beneka et al. - 2005 - Resistance training effects on muscular strength o.pdf}
}

